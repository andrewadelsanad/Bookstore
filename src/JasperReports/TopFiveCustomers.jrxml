    <queryString language="SQL">
        <![CDATA[SELECT User.* , SUM(book.no_copies) AS sum_copies,
 SUM(book.no_copies * Book.price) AS sum_paid
 FROM Sale,Book,User
 WHERE   YEAR(sales.purchase_date) >= YEAR(CURRENT_DATE - INTERVAL 3 MONTH)
AND YEAR(sales.purchase_date) <= YEAR(CURRENT_DATE)
AND MONTH(sales.purchase_date) >= MONTH(CURRENT_DATE - INTERVAL 3 MONTH)
AND MONTH(sales.purchase_date) <= MONTH(CURRENT_DATE)
AND sales.ISBN=Book.ISBN AND sales.user_name = users.user_name
GROUP BY users.user_name
ORDER BY sum_paid DESC
LIMIT 5]]>
    </queryString>
